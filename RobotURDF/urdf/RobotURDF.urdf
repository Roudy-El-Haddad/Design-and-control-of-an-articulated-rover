<!-- Sensors --> 
  <gazebo>
    <plugin name="towr_gps" filename="libhector_gazebo_ros_gps.so">
      <alwaysOn>true</alwaysOn>
      <updateRate>1000</updateRate>
      <bodyName>base_link</bodyName> <!-- must be the link of the gps device -->
      <frameId>base_link</frameId>
      <topicName>/RobotURDF/GPS/fix</topicName>
      <!-- Robotnik position at Fuente del Jarro -->
      <referenceLatitude>0</referenceLatitude>
      <referenceLongitude>0</referenceLongitude>
      <!-- To set heading in ENU orientation (degrees) -->
      <!-- <referenceHeading>90</referenceHeading> -->
      <velocityTopicName>/RobotURDF/GPS/fix_velocity</velocityTopicName>
      <drift>0.0 0.0 0.0</drift>
      <!--<drift>0.0001 0.0001 0.0001</drift>-->
      <!--<drift>0.3 0.3 0.3</drift>-->
      <gaussianNoise>0.0 0.0 0.0</gaussianNoise>
      <!--<gaussianNoise>0.00001 0.00001 0.00001</gaussianNoise>-->
      <velocityDrift>0.00000 0.00000 0.00000</velocityDrift>
      <!--<velocityGaussianNoise>0.1 0.1 0.1</velocityGaussianNoise>-->
      <velocityGaussianNoise>0.00000 0.00000 0.00000</velocityGaussianNoise>
      <ignoreTfPrefix>1</ignoreTfPrefix>
    </plugin>
  </gazebo>

    <gazebo>
      <plugin filename="libhector_gazebo_ros_imu.so" name="gazebo_ros_imu">
        <alwaysOn>true</alwaysOn>
        <bodyName>base_link</bodyName>
        <updateRate>1000</updateRate>
        <serviceName>/RobotURDF/imu_service</serviceName>
        <topicName>/RobotURDF/imu</topicName>
        <gaussianNoise>0</gaussianNoise>
        <xyzOffset>0 0 0</xyzOffset>
        <rpyOffset>0 0 0</rpyOffset>
      </plugin>
    </gazebo>

    <gazebo>
      <plugin name="p3d_base_controller" filename="libgazebo_ros_p3d.so">
      <alwaysOn>true</alwaysOn>
      <updateRate>1000</updateRate>
      <bodyName>base_link</bodyName>
      <topicName>/RobotURDF/base_pose_ground_truth</topicName>
      <gaussianNoise>0</gaussianNoise>
      <!--<frameName>/map</frameName>-->
       <xyzOffsets>0 0 0</xyzOffsets>
      <rpyOffsets>0 0 0</rpyOffsets>
      </plugin>
   </gazebo>

